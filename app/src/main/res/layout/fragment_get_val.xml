<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context=".ui.get_value.GetValueFragment">

<LinearLayout

    android:id="@+id/Sams_data_table"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/holo_blue_bright">

    <TableLayout
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="10dip"
        android:layout_marginRight="10dip"
        android:layout_marginBottom="10dip">

        <TableRow
            android:id="@+id/lable_Header"
            android:layout_width="match_parent"
            android:layout_height="match_parent">


            <TextView
                android:id="@+id/farmName"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:hint="농장 이름"
                android:layout_margin="2dp"
                android:gravity="center"
                android:textSize="20sp"
                android:textColor="@color/black"
                android:background="#FFF59D"/>`
            <TextView
                android:id="@+id/currentVal"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:hint="현재 값 조회"
                android:layout_margin="2dp"
                android:gravity="center"
                android:textSize="20sp"
                android:textColor="@color/black"
                android:background="#FFF59D"/>`


        </TableRow>
        <TableRow
            android:id="@+id/equipment"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ToggleButton
                android:id="@+id/equipment1"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_margin="5dp"
                android:gravity="center"
                android:textOn="버튼 1"
                android:textOff="버튼 1"
                android:textColor="@color/black"
                android:textSize="20sp"
                android:background="@drawable/button"/>

            <ToggleButton
                android:id="@+id/equipment2"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:textOn="버튼 2"
                android:textOff="버튼 2"
                android:layout_margin="5dp"
                android:gravity="center"
                android:textSize="20sp"
                android:textColor="@color/black"
                android:background="@drawable/button"/>
            <ToggleButton
                android:id="@+id/equipment3"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:textOn="버튼 3"
                android:textOff="버튼 3"
                android:layout_margin="5dp"
                android:gravity="center"
                android:textSize="20sp"
                android:textColor="@color/black"
                android:background="@drawable/button"
                />
            <ToggleButton
                android:id="@+id/equipment4"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:textOn="버튼 4"
                android:textOff="버튼 4"
                android:layout_margin="5dp"
                android:gravity="center"
                android:textSize="20sp"
                android:textColor="@color/black"
                android:background="@drawable/button"/>
        </TableRow>
